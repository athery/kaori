[{sasl,[{errlog_type,error}]},
 {logger,
     [{console,
          [{format,<<"$time $metadata[$level] $message\n">>},
           {metadata,[request_id]}]},
      {level,info}]},
 {kaori,
     [{ecto_repos,['Elixir.Kaori.Repo']},
      {'Elixir.KaoriWeb.Endpoint',
          [{render_errors,
               [{view,'Elixir.KaoriWeb.ErrorView'},
                {accepts,[<<"html">>,<<"json">>]}]},
           {pubsub,
               [{name,'Elixir.Kaori.PubSub'},
                {adapter,'Elixir.Phoenix.PubSub.PG2'}]},
           {load_from_system_env,true},
           {server,true},
           {secret_key_base,<<"${SECRET_KEY_BASE}">>},
           {url,[{host,<<"kaori.gigalixirapp.com">>},{port,80}]},
           {cache_static_manifest,<<"priv/static/cache_manifest.json">>}]},
      {'Elixir.Kaori.Repo',
          [{adapter,'Elixir.Ecto.Adapters.Postgres'},
           {url,<<"${DATABASE_URL}">>},
           {database,<<>>},
           {ssl,true},
           {pool_size,1}]}]}].
